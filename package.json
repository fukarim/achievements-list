{
  "name": "achievements-list",
  "description": "Achievements list",
  "version": "0.0.1",
  "scripts": {
    "start-server:development": "cross-env NODE_ENV=development NODE_ICU_DATA=node_modules/full-icu nodemon ./src/server/server.js --watch src/server --watch public/App.js",
    "start-server:production": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu DATA_FILE=achievements.json node ./src/server/server.js",
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "full-icu": "^1.3.0",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "nodemon": "^1.19.1",
    "rollup": "^1.16.4",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "svelte": "^3.0.0",
    "svelte-routing": "^1.2.1",
    "terser": "^4.0.2"
  }
}
